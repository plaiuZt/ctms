@using CTMS.Entity;
@model CTMS.Entity.SysUpdateLog
@{ Layout = ""; }

<form class="layui-form" action="" id="frm_SysUpdateLogAdd">
    <div class="form-module-content">
        <div class="layui-form-item">
            <label class="layui-form-label"> </label>
            <div class="layui-input-block">
                @(string.IsNullOrEmpty(ViewBag.OldVersionName) ? "" : ("上次版本号：" + ViewBag.OldVersionName))
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">版本号 </label>
            <div class="layui-input-block">
                @Html.HiddenFor(m => m.Id)
                @Html.HiddenFor(m => m.Status)
                @Html.HiddenFor(m => m.CreateBy)
                @Html.HiddenFor(m => m.CreateDt)
                @Html.TextBoxFor(m => m.VersionNum, "", new Dictionary<string, object> { { "autocomplete", "off" }, { "class", "layui-input  " }, { "lay-verify", "title" }, { "placeholder", "请输入版本号" } })
            </div>
        </div>

        <div class="layui-form-item">
            <label class="layui-form-label">版本说明</label>
            <div class="layui-input-block">
                @Html.TextAreaFor(m => m.Remark, new Dictionary<string, object> { { "class", "layui-textarea" }, { "maxlength", 500 }, { "data-msg-maxlength", "最大长度为:500" } })
            </div>
        </div>
    </div>
    @*<div class="form-module-footer">
            <button class="layui-btn" lay-submit lay-filter="formDemo">立即提交</button>
        </div>*@
</form>